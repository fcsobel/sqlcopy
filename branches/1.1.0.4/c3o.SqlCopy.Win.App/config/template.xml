<ArrayOfCopyObject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <CopyObject>
    <Name>SqlServer</Name>
	  <SourceType>SqlServer</SourceType>
    <Source></Source>
    <Destination></Destination>
    <BatchSize>0</BatchSize>
